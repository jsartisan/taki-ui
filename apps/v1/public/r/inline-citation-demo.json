{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "inline-citation-demo",
  "type": "registry:example",
  "dependencies": [
    "ai"
  ],
  "registryDependencies": [
    "https://taki-ui.com/r/inline-citation.json",
    "https://taki-ui.com/r/hover-card.json",
    "https://taki-ui.com/r/badge.json",
    "https://taki-ui.com/r/carousel.json"
  ],
  "files": [
    {
      "path": "registry/new-york/examples/inline-citation-demo.tsx",
      "content": "import {\n  InlineCitation,\n  InlineCitationCard,\n  InlineCitationCardBody,\n  InlineCitationCardTrigger,\n  InlineCitationCarousel,\n  InlineCitationCarouselContent,\n  InlineCitationCarouselHeader,\n  InlineCitationCarouselIndex,\n  InlineCitationCarouselItem,\n  InlineCitationCarouselNext,\n  InlineCitationCarouselPrev,\n  InlineCitationSource,\n  InlineCitationText,\n} from \"@/registry/new-york/ai-elements/inline-citation\"\n\nexport default function InlineCitationDemo() {\n  const sources = [\n    \"https://react.dev\",\n    \"https://developer.mozilla.org\",\n    \"https://www.typescriptlang.org\",\n  ]\n\n  return (\n    <div className=\"w-full max-w-2xl\">\n      <p className=\"text-sm leading-relaxed\">\n        <InlineCitation>\n          <InlineCitationText>\n            React is a JavaScript library for building user interfaces\n          </InlineCitationText>\n          <InlineCitationCard>\n            <InlineCitationCardTrigger sources={sources} />\n            <InlineCitationCardBody>\n              <InlineCitationCarousel>\n                <InlineCitationCarouselHeader>\n                  <InlineCitationCarouselPrev />\n                  <InlineCitationCarouselNext />\n                  <InlineCitationCarouselIndex />\n                </InlineCitationCarouselHeader>\n                <InlineCitationCarouselContent>\n                  <InlineCitationCarouselItem>\n                    <InlineCitationSource\n                      title=\"React Documentation\"\n                      url=\"https://react.dev\"\n                      description=\"The official React documentation provides comprehensive guides and API references for building user interfaces.\"\n                    />\n                  </InlineCitationCarouselItem>\n                  <InlineCitationCarouselItem>\n                    <InlineCitationSource\n                      title=\"MDN Web Docs\"\n                      url=\"https://developer.mozilla.org\"\n                      description=\"Mozilla's comprehensive web development documentation covering HTML, CSS, JavaScript, and Web APIs.\"\n                    />\n                  </InlineCitationCarouselItem>\n                  <InlineCitationCarouselItem>\n                    <InlineCitationSource\n                      title=\"TypeScript Docs\"\n                      url=\"https://www.typescriptlang.org\"\n                      description=\"Official TypeScript documentation and tutorials for learning typed JavaScript.\"\n                    />\n                  </InlineCitationCarouselItem>\n                </InlineCitationCarouselContent>\n              </InlineCitationCarousel>\n            </InlineCitationCardBody>\n          </InlineCitationCard>\n        </InlineCitation>\n        , developed and maintained by Meta (formerly Facebook).\n      </p>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}