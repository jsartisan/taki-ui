{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "tooltip-placement",
  "type": "registry:example",
  "registryDependencies": [
    "tooltip"
  ],
  "files": [
    {
      "path": "registry/v1/examples/tooltip-placement.tsx",
      "content": "import { Button } from \"@/registry/new-york-v4/ui/button\"\nimport { Tooltip, TooltipTrigger } from \"@/registry/new-york-v4/ui/tooltip\"\n\nexport default function TooltipPlacement() {\n  return (\n    <div className=\"flex flex-wrap items-center justify-center gap-4\">\n      <TooltipTrigger>\n        <Button variant=\"outline\">Top</Button>\n        <Tooltip placement=\"top\">Tooltip on top</Tooltip>\n      </TooltipTrigger>\n\n      <TooltipTrigger>\n        <Button variant=\"outline\">Bottom</Button>\n        <Tooltip placement=\"bottom\">Tooltip on bottom</Tooltip>\n      </TooltipTrigger>\n\n      <TooltipTrigger>\n        <Button variant=\"outline\">Left</Button>\n        <Tooltip placement=\"left\">Tooltip on left</Tooltip>\n      </TooltipTrigger>\n\n      <TooltipTrigger>\n        <Button variant=\"outline\">Right</Button>\n        <Tooltip placement=\"right\">Tooltip on right</Tooltip>\n      </TooltipTrigger>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}