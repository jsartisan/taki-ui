{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "sidebar-footer",
  "files": [
    {
      "path": "registry/new-york/internal/sidebar-footer.tsx",
      "content": "\"use client\"\n\nimport { ChevronUpIcon } from \"lucide-react\"\n\nimport { Menu, MenuItem, MenuTrigger } from \"@/registry/new-york/ui/menu\"\nimport {\n  Sidebar,\n  SidebarContent,\n  SidebarFooter,\n  SidebarHeader,\n  SidebarInset,\n  SidebarMenu,\n  SidebarMenuButton,\n  SidebarMenuItem,\n  SidebarProvider,\n  SidebarTrigger,\n} from \"@/registry/new-york/ui/sidebar\"\n\nexport default function AppSidebar() {\n  return (\n    <SidebarProvider>\n      <Sidebar>\n        <SidebarHeader />\n        <SidebarContent />\n        <SidebarFooter>\n          <SidebarMenu>\n            <SidebarMenuItem>\n              <MenuTrigger>\n                <SidebarMenuButton className=\"data-[pressed]:bg-sidebar-accent data-[pressed]:text-sidebar-accent-foreground\">\n                  Username\n                  <ChevronUpIcon className=\"ml-auto\" />\n                </SidebarMenuButton>\n                <Menu placement=\"top\" className=\"w-(--trigger-width)\">\n                  <MenuItem>\n                    <span>Account</span>\n                  </MenuItem>\n                  <MenuItem>\n                    <span>Billing</span>\n                  </MenuItem>\n                  <MenuItem>\n                    <span>Sign out</span>\n                  </MenuItem>\n                </Menu>\n              </MenuTrigger>\n            </SidebarMenuItem>\n          </SidebarMenu>\n        </SidebarFooter>\n      </Sidebar>\n      <SidebarInset>\n        <header className=\"flex h-12 items-center justify-between px-4\">\n          <SidebarTrigger />\n        </header>\n      </SidebarInset>\n    </SidebarProvider>\n  )\n}\n",
      "type": "registry:component"
    }
  ],
  "type": "registry:internal"
}