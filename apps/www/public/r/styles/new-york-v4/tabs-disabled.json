  "files": [
    {
      "path": "registry/new-york-v4/examples/tabs-disabled.tsx",
      "content": "import {\n  Tabs,\n  TabPanel,\n  TabsList,\n  TabsTrigger,\n} from \"@/registry/new-york-v4/ui/tabs\"\n\nexport default function TabsDisabled() {\n  return (\n    <Tabs defaultSelectedKey=\"general\">\n      <TabsList aria-label=\"Settings\">\n        <TabsTrigger id=\"general\">General</TabsTrigger>\n        <TabsTrigger id=\"billing\" isDisabled>\n          Billing\n        </TabsTrigger>\n        <TabsTrigger id=\"team\">Team</TabsTrigger>\n      </TabsList>\n      <TabPanel id=\"general\">\n        <p className=\"text-sm text-muted-foreground\">\n          General settings and preferences.\n        </p>\n      </TabPanel>\n      <TabPanel id=\"billing\">\n        <p className=\"text-sm text-muted-foreground\">\n          Billing information and subscription details.\n        </p>\n      </TabPanel>\n      <TabPanel id=\"team\">\n        <p className=\"text-sm text-muted-foreground\">\n          Manage your team members and their permissions.\n        </p>\n      </TabPanel>\n    </Tabs>\n  )\n}\n\n",
      "type": "registry:example"
    }
  ]
}