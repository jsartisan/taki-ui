  "files": [
    {
      "path": "registry/v1/examples/breadcrumb-demo.tsx",
      "content": "\"use client\"\n\nimport {\n  Breadcrumb,\n  BreadcrumbEllipsis,\n  BreadcrumbLink,\n  Breadcrumbs,\n  BreadcrumbSeparator,\n} from \"@/registry/new-york-v4/ui/breadcrumb\"\nimport { Button } from \"@/registry/new-york-v4/ui/button\"\nimport { Menu, MenuItem, MenuTrigger } from \"@/registry/new-york-v4/ui/menu\"\n\nexport default function BreadcrumbDemo() {\n  return (\n    <Breadcrumbs>\n      <Breadcrumb>\n        <BreadcrumbLink href=\"/\">Home</BreadcrumbLink>\n        <BreadcrumbSeparator />\n      </Breadcrumb>\n      <Breadcrumb>\n        <MenuTrigger>\n          <Button\n            variant=\"ghost\"\n            className=\"flex h-auto items-center gap-1 p-0 hover:bg-transparent\"\n          >\n            <BreadcrumbEllipsis className=\"size-4\" />\n            <span className=\"sr-only\">Toggle menu</span>\n          </Button>\n          <Menu placement=\"bottom start\">\n            <MenuItem>Documentation</MenuItem>\n            <MenuItem>Themes</MenuItem>\n            <MenuItem>GitHub</MenuItem>\n          </Menu>\n        </MenuTrigger>\n        <BreadcrumbSeparator />\n      </Breadcrumb>\n      <Breadcrumb>\n        <BreadcrumbLink href=\"/docs/components\">Components</BreadcrumbLink>\n        <BreadcrumbSeparator />\n      </Breadcrumb>\n      <Breadcrumb>\n        <BreadcrumbLink>Breadcrumb</BreadcrumbLink>\n      </Breadcrumb>\n    </Breadcrumbs>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}