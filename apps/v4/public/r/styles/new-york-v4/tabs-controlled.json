{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "tabs-controlled",
  "type": "registry:example",
  "registryDependencies": [
    "tabs",
    "button"
  ],
  "files": [
    {
      "path": "registry/new-york-v4/examples/tabs-controlled.tsx",
      "content": "\"use client\"\n\nimport { useState } from \"react\"\n\nimport { Button } from \"@/registry/new-york-v4/ui/button\"\nimport {\n  Tabs,\n  TabPanel,\n  TabsList,\n  TabsTrigger,\n} from \"@/registry/new-york-v4/ui/tabs\"\n\nexport default function TabsControlled() {\n  const [selectedTab, setSelectedTab] = useState(\"overview\")\n\n  return (\n    <div className=\"space-y-4\">\n      <div className=\"flex gap-2\">\n        <Button\n          variant=\"outline\"\n          size=\"sm\"\n          onPress={() => setSelectedTab(\"overview\")}\n        >\n          Go to Overview\n        </Button>\n        <Button\n          variant=\"outline\"\n          size=\"sm\"\n          onPress={() => setSelectedTab(\"analytics\")}\n        >\n          Go to Analytics\n        </Button>\n      </div>\n      <Tabs\n        selectedKey={selectedTab}\n        onSelectionChange={(key) => setSelectedTab(key as string)}\n      >\n        <TabsList aria-label=\"Dashboard sections\">\n          <TabsTrigger id=\"overview\">Overview</TabsTrigger>\n          <TabsTrigger id=\"analytics\">Analytics</TabsTrigger>\n          <TabsTrigger id=\"reports\">Reports</TabsTrigger>\n        </TabsList>\n        <TabPanel id=\"overview\">\n          <p className=\"text-sm text-muted-foreground\">\n            Overview of your dashboard with key metrics.\n          </p>\n        </TabPanel>\n        <TabPanel id=\"analytics\">\n          <p className=\"text-sm text-muted-foreground\">\n            Detailed analytics and insights.\n          </p>\n        </TabPanel>\n        <TabPanel id=\"reports\">\n          <p className=\"text-sm text-muted-foreground\">\n            Generate and view reports.\n          </p>\n        </TabPanel>\n      </Tabs>\n    </div>\n  )\n}\n\n",
      "type": "registry:example"
    }
  ]
}